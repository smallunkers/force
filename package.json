{
  "name": "force",
  "version": "1.0.0",
  "description": "通用组件脚手架",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "concurrently 'npm run build:dist' 'npm run tsc' 'npm run gulp'",
    "build:dist": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "tsc": "tsc && cp -a src/images lib 2>/dev/null || :",
    "gulp": "gulp",
    "analyzer": "webpack-bundle-analyzer dist/stats.json",
    "eslint": "eslint . --ext .ts,.tsx",
    "es:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "npm run eslint",
        "git add"
      ]
    }
  },
  "main": "index.js",
  "typings": "lib/index.d.ts",
  "license": "ISC",
  "author": "qiyou.lqy",
  "dependencies": {
    "d3": "5.7.0",
    "d3-fetch": "1.1.2",
    "html2canvas": "^1.0.0-rc.5",
    "lodash": "^4.17.15",
    "moment": "2.24.0",
    "react": "16.7.0",
    "react-dom": "16.7.0"
  },
  "devDependencies": {
    "@types/d3": "5.7.0",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.18",
    "@types/react": "16.8.5",
    "@types/react-dom": "16.8.2",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "autoprefixer": "9.4.7",
    "awesome-typescript-loader": "5.2.1",
    "antd": "3.13.6",
    "babel-eslint": "10.0.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.22.0",
    "babel-runtime": "6.22.0",
    "clean-webpack-plugin": "1.0.1",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "6.0.0",
    "gulp-stylus": "2.7.0",
    "husky": "1.3.1",
    "json-loader": "0.5.7",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "postcss-safe-parser": "4.0.1",
    "react-hot-loader": "4.12.15",
    "source-map-loader": "0.2.4",
